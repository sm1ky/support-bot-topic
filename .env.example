# Название бота в главном меню
BOT_NAME=Support Bot

# API конфигурация
# Username бота (без @)
BOT_USERNAME=your_bot_username

# Конфигурация Redis
# Хост Redis сервера
REDIS_HOST=redis
# Порт Redis сервера
REDIS_PORT=6379
# Номер базы данных Redis
REDIS_DB=0