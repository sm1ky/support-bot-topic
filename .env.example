# Конфигурация бота
BOT_TOKEN=your_bot_token_here
# ID разработчика (ваш Telegram user ID)
BOT_DEV_ID=123456789
# ID группы для поддержки (group chat ID)
BOT_GROUP_ID=-1001234567890
# ID кастомного эмодзи для темы группы [Only premium bots]
